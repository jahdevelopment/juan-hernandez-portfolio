{
  "name": "juan-hernandez-personal-portfolio",
  "scripts": {
    "build:css": "cleancss src/css/*.css -o dist/css/style.css",
    "build:cssprojects": "cleancss src/projects/styles/*.css -o dist/projects/styles/styles.css",
    "copy:html": "cp src/*.html dist",
    "copy:html-projects": "cp src/projects/*.html dist/projects",
    "minify-images": "imagemin src/images/* --out-dir=dist/images",
    "build:js": "mkdirp dist/js && uglifyjs src/js/*.js -o dist/js/main.js",
    "build:jsprojects": "mkdirp dist/projects/js && uglifyjs src/projects/project-scripts/*.js -o dist/projects/js/script.js",
    "minify-project-images1": "imagemin src/projects/images/drumKItImg/* --out-dir=dist/projects/images/drumKitImg",
    "minify-project-images2": "imagemin src/projects/images/pigGameImg/* --out-dir=dist/projects/images/pigGameImg",
    "build:all": "npm run build:css && npm run build:cssprojects && npm run copy:html && npm run copy:html-projects && npm run minify-images && npm run build:js && npm run build:jsprojects && npm run minify-project-images1 && npm run minify-project-images2" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanhernandezdeveloper/juan-hernandez-portfolio.git"
  },
  "description": "Juan Alberto Hernandez - Personal Portfolio.",
  "bugs": {
    "url": "https://github.com/juanhernandezdeveloper/juan-hernandez-portfolio/issues"
  },
  "homepage": "https://github.com/juanhernandezdeveloper/juan-hernandez-portfolio#readme",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "rollup": "^2.70.0"
  },
  "devDependencies": {
    "clean-css-cli": "^5.5.2",
    "imagemin-cli": "^7.0.0",
    "uglify-js": "^3.15.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
